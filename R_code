t<- t%>% mutate(diff=ymd(prescdate)-ymd(admidate) )
t <- t %>% group_by(patid) %>% arrange(patid,diff)
t$i_1825<- ifelse(!is.na(t$diff)&t$diff>0&t$diff<1825,1,0)
